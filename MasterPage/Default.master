<%@ Master Language="C#" AutoEventWireup="true" CodeFile="Default.master.cs" Inherits="_Default" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title></title>
    <link href="../default.css" rel="stylesheet" type="text/css" />
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body class="main_body_back_frame">
    <form id="form1" runat="server">
    <asp:ScriptManager ID="script_manager_main" runat="server">
    </asp:ScriptManager>
    <center>
        <asp:Panel runat="server" ID="panel_header" CssClass="HeadBox">
            <asp:Label runat="server" ID="lbl_header_text" Text="Shwe Yoe" ForeColor="#4387FE"
                Font-Overline="false" Font-Bold="true" Font-Size="24" />
        </asp:Panel>
        <ajaxToolkit:RoundedCornersExtender ID="RoundedCornersExtender_panelHeader" runat="server"
            TargetControlID="panel_header">
        </ajaxToolkit:RoundedCornersExtender>
        <asp:Panel runat="server" ID="panel_menu" CssClass="MenuBox">
            <center>
                <table width="1000" style="text-align:left">
                    <tr valign="middle">
                        <td style="width:64px">
                            <asp:Image ID="Image1" runat="server" ImageUrl="~/Images/find_rooms.png" />
                        </td>
                        <td>
                            <asp:LinkButton ID="LinkButton1" runat="server" Text="Find Room" SkinID="menu" 
                                PostBackUrl="~/list" />
                        </td>
                        <td  style="width:64px">
                            <asp:Image ID="Image2" runat="server" ImageUrl="~/Images/post_adv.png" />
                        </td>
                        <td>
                            <asp:LinkButton ID="LinkButton2" runat="server" Text="Post Room Advertisement" 
                                SkinID="menu" PostBackUrl="~/post" />
                        </td>
                    </tr>
                </table>
            </center>
        </asp:Panel>
        <asp:Panel runat="server" ID="panel_body" CssClass="main_body_frame">
            <table>
                <tr>
                    <td>
                        <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
                        </asp:ContentPlaceHolder>
                    </td>
                </tr>
            </table>
            <asp:UpdateProgress runat="server" ID="update_progress_master">
                <ProgressTemplate>
                    <asp:Panel runat="server" ID="panel1" CssClass="panel_progress_overlay">
                        <asp:Panel runat="server" ID="panel" CssClass="panel_progress">
                            <table style="width: 100%; height: 100%;">
                                <tr align="center">
                                    <td valign="middle" align="right" style="width: 30%">
                                        <img src="../Images/loadingAnimation.gif" alt="Loading" width="32px" height="32px" />
                                    </td>
                                    <td valign="middle" align="left" style="width: 70%">
                                        <asp:Label runat="server" ID="lbl_updating" Text="Processing, Please wait...." />
                                    </td>
                                </tr>
                            </table>
                        </asp:Panel>
                    </asp:Panel>
                </ProgressTemplate>
            </asp:UpdateProgress>
        </asp:Panel>
        <ajaxToolkit:RoundedCornersExtender ID="RoundedCornersExtender_body" runat="server"
            TargetControlID="panel_body">
        </ajaxToolkit:RoundedCornersExtender>
    </center>
    </form>
</body>
</html>
